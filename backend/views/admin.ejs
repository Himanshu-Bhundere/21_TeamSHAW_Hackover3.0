        <div class="col py-3">
          <!-- DATA CARDS -->
      <div id="cards">
        <div class="users-count">
          <div class="card" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title"><img id="user-icon" src="./user-icon.png" alt="oops" width="20" height="20"> Users</h5>
                <h2 class="card-subtitle mb-2 text-muted">10,000</h2>
                <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                <a href="#" class="card-link">Card link</a>
                <a href="#" class="card-link">Another link</a> -->
              </div>
            </div>
        </div>
        <div class="events-count">
          <div class="card" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title"><img id="user-icon" src="./calender.png" alt="oops" width="25" height="20"> Events</h5>
                <h2 class="card-subtitle mb-2 text-muted">200</h2>
              </div>
            </div>
        </div>
        <div class="tot-rev">
          <div class="card" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title"><img id="user-icon" src="./dollar-icon.png" alt="oops" width="25" height="20"> Total revenue</h5>
                <h2 class="card-subtitle mb-2 text-muted">1,20,000</h2>
              </div>
            </div>
          </div>
        </div><br><br><br><br><br>
        <h2>Responsive Table</h2>
<div class="table-wrapper">
    <table class="fl-table">
        <thead>
        <tr>
            <th>Organization Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Event Name</th>
            <th>Location</th>
            <th>Ticket</th>
          </tr>
        </thead>
        <tbody>
        <% for(let org of organs){ %>
        <tr>
            <td><%= org.nameOrg %></td>
            <td><%= org.phone %></td>
            <td><%= org.email %></td>
            <td><%= org.eventName %></td>
            <td><%= org.location %></td>
            <td><%= org.ticket %></td>
        </tr>
        <% } %>
        <tbody>
    </table>
